{
    "name": "gcode/wordpress-base",
    "description": "WordPress base site with some plugins",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "aws/aws-sdk-php":"*",
        "wpackagist-plugin/akismet":"dev-trunk",
        "wpackagist-plugin/amazon-web-services":"dev-trunk",
        "wpackagist-plugin/amazon-s3-and-cloudfront":"dev-trunk",
        "wpackagist-plugin/captcha":"dev-trunk",
        "wpackagist-plugin/facebook":"dev-trunk",
        "wpackagist-plugin/facebook-publish":"dev-trunk",
        "wpackagist-plugin/wp-google-analytics":"dev-trunk",
        "wpackagist-plugin/wp-mail-smtp": "dev-trunk",
	"wpackagist-plugin/json-api": "1.1.3",
        "fancyguy/wordpress-monolog": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/"
        }
    }
}
